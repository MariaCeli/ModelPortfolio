            }

            function getWidthPercent() {
              var windowWidth = window.innerWidth;
              return largeGridSizes(windowWidth);
            }

            widthPercent = getWidthPercent();
            var Resizer = {};
            $(window).resize(function() {
              widthPercent = getWidthPercent();
              Resizer.gridResizer(widthPercent);
            });
            var g_height = 0;
            (Resizer.gridResizer = function (widthPercent) {
              $('#photo-gallery-4371420 .grid-item').each(function(){
                var $this = $(this);

                $this.css({
                 'width' : "calc("+widthPercent+" - "+ gutterValueRes +"px)"
                });

                g_height = $this.width();
                $this.height(parseInt(g_height));
                $this.css({
                  'margin-bottom': gutterValue,
                });
              });

            })(widthPercent);
            $grid.isotope({
              itemSelector: '#photo-gallery-4371420 .grid-item',
              percentPosition: true,
              transitionDuration: '0s',
              masonry: {
              columnWidth: '#photo-gallery-4371420 .grid-sizer',
              gutter: gutterValue,
            }
            });
          };
          gridInit();
        })();
      } else if ( gallerylayout_4371420 == 'horizontal_grid_large' ) {
        (IsotopeInit_4371420.horizontalGridLarge = function (){
          var gutterValue, widthPercent, gutterValueRes;
          var gridInit = function() {
            var $grid = $('#photo-gallery-4371420 .grid');
            gutterValue = grid_gutter;
            gutterValueRes = grid_gutter;
            if( window.innerWidth <= 1024 ) {
              gutterValue = grid_gutter_mobile;
              extraOffset = 0
              gutterValueRes = grid_gutter_mobile
            }

            if (window.innerWidth <= 767) {
              gutterValueRes = (layout_mobile_4371420 == 1 && window.matchMedia("(orientation: portrait)").matches) ? 0 : grid_gutter_mobile;
            }

            var $photoGallery = $('#photo-gallery-4371420');
            if ($photoGallery.parents('.is-section-fullwidth_inset, .is-section-pagewith_max').length == 1) {
              if( window.innerWidth > 767 ) {
                $photoGallery.find('.js-grid-padding').css({
                  'padding-left': '0',
                  'margin-right': -gutterValue
                });
              } else{
                if( layout_mobile_4371420 == 2 ) {
                  $photoGallery.find('.js-grid-padding').css({
                    'margin-right': -grid_gutter_mobile
                  });
                }
              }

              $photoGallery.find('.js-grid').css('padding-top', '0');
            } else{
              if (isBlog == 1) {
                $photoGallery.find('.js-grid-padding').css({
                  'padding-left': '0',
                  'margin-right': -gutterValue
                });
                $photoGallery.find('.js-grid').css('padding-top', '0');
              } else {
                if( window.innerWidth > 767 ) {
                  $photoGallery.find('.js-grid-padding').css({
                    'padding-left': gutterValue,
                  });
                } else{
                  if( layout_mobile_4371420 == 2 ) {
                    $photoGallery.find('.js-grid-padding').css({
                      'margin-right': -grid_gutter_mobile
                    });
                  }
                }

                $photoGallery.find('.js-grid').css('padding-top', gutterValue);
              }
            }

