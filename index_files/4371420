              $('.slidee').find('li').removeClass('prev_slide');
              $('.slidee').find('li').removeClass('next_slide');

              if( $(parent).find('iframe').length == 1) {
                $(parent).find('iframe').remove();
                $(parent).find('img').show();
                $(parent).find('.js-mediatype-video-play').show();   
              }
              var parent1 = $('#photo-gallery-4371420 #frame #slidee-4371420 li')[itemIndex + 1];
              if( $(parent1).find('iframe').length == 1) {
                $(parent1).find('iframe').remove();
                $(parent1).find('img').show();
                $(parent1).find('.js-mediatype-video-play').show();
              }

              $(parent).addClass('prev_slide');
              $(parent1).addClass('next_slide');

              setTimeout(function(){
                $('#photo-gallery-4371420 #frame #slidee-4371420 li').removeClass('js-slideshow-item');
                $('#photo-gallery-4371420 #frame #slidee-4371420 li').find('.js-hoz-photo-info-btn,.js-photo-info-wrapper').removeClass('active');
                $('#photo-gallery-4371420 .list-active').addClass('js-slideshow-item');
                $('#photo-gallery-4371420 #frame #slidee-4371420 li').addClass('js-horizontal_slider-destroy');
