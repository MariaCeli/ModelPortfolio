    var tempscroll, loaderCountValue = $('#total-photocount').val();
    $(window).on('scroll', function(event) {
      tempscroll = window.scrollY;
      //---------------
      unveilImages(5);
      //---------------
      if( window.navigator.userAgent.indexOf('Trident/'))
        tempscroll = window.pageYOffset;

      if(((tempscroll + 500)+ window.innerHeight) >= (getDocHeight()-getDocHeight()*0.25)/2) {
        var galleryId, temp, CDN_S3 = "https://s3-img.pixpa.com/";
        var csrf_token = $('#csrf_token').val();

        if( window.nextKey_4371420 < window.totalkeys_4371420 - 1 ) {
          var pagination = {};
          var newdata = getNextPageData_4371420();
          (pagination.load = function(data) {
            var $gridArray = data[0];
            $('#photo-gallery-4371420 .js-gridtemp-holder_4371420').html('');
            $.each($gridArray, function( key, value ) {
              temp = $("#photogallery-template_4371420").html();
              temp = $(temp)[0];
              var newData = $gridArray[key];

              if( gallerylayout_4371420 == 'pinterest_grid_detailed' || gallerylayout_4371420 == 'three_two_grid' || gallerylayout_4371420 == 'square_block_grid' || gallerylayout_4371420 == 'pinterest_grid_large_detailed' ){
                if( newData.type == 3 )
                  $(temp).find('.js-grid-image').addClass('bg-photo-color');
                else
                  $(temp).find('.js-grid-image').css('background-color', newData.color );
              } else if (gallerylayout_4371420 == 'spaced_out_small' || gallerylayout_4371420 == 'spaced_out_medium' || gallerylayout_4371420 == 'spaced_out_large') {
                if( newData.type == 3 )
                  $(temp).find('.js-grid-image').addClass('bg-photo-color');
                else
                  $(temp).find('.js-grid-image').css('background-color', newData.color );
              } else {
                if( newData.type == 3 )
                  $(temp).addClass('bg-photo-color');
                else
                  $(temp).css('background-color', newData.color );
              }
              $(temp).attr('data-gridimageId', newData.id);
              $(temp).attr('data-videotype', newData.thumbpath);
              $(temp).attr('data-mediatype', newData.type);
              $(temp).attr('data-imageurl', newData.userid+'-photo-'+key);
              $(temp).attr('data-height', newData.height);
              $(temp).attr('data-width', newData.width);
              $(temp).attr('data-index', key);
              $(temp).attr('data-color', newData.color);
              $(temp).attr('data-key', newData.id);
              $(temp).attr('data-photoid', newData.id);
              $(temp).attr('data-photo_versionid', Date.parse(newData.updated_at));
              $(temp).attr('data-watermark_versionid', '');
              $(temp).attr('data-version', newData.version);
              // if(newData.version == 1){
              //   $(temp).attr('data-watermark', '0');  
              //   watermark = 0;
              // } else{
              //   $(temp).attr('data-watermark', newData.watermark);
              //   watermark = newData.watermark;
              // }              

              var galleryWatermark = "0";
              if(newData.version == 1  && newData.watermark == 1){
                $(temp).attr('data-watermark', '0');
