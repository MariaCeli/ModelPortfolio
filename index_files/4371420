                    });
                  } else{
                    $photoGallery.find('.js-grid-padding').css({
                      'padding-left': '',
                      'margin-right': ''
                    });
                  }
                }
              }

              $photoGallery.find('.js-grid').css('padding-top', '0');

            } else{
              if (isBlog == 1) {
                $photoGallery.find('.js-grid-padding').css({
                  'padding-left': '0',
                  'margin-right': -gutterValue
                });

                $photoGallery.find('.js-grid').css('padding-top', '0');

              } else {

                if( window.innerWidth >= 767 ) {
                  $photoGallery.find('.js-grid-padding').css({
                    'padding-left': gutterValue,
                  });
                } else{

                  if (window.matchMedia("(orientation: landscape)").matches) {
                    $photoGallery.find('.js-grid-padding').css({
                      'margin-right': -grid_gutter_mobile
                    });

                  } else{
                    if( layout_mobile_4371420 == 2 || layout_mobile_4371420 == 3 ) {
                      $photoGallery.find('.js-grid-padding').css({
                        'margin-right': -grid_gutter_mobile
                      });
                    } else{
                      $photoGallery.find('.js-grid-padding').css({
                        'padding-left': '',
                        'margin-right': ''
                      });
                    }
                  }
                }

                $photoGallery.find('.js-grid').css('padding-top', gutterValue);
              }
            }

            function gp() {
              var windowWidth = window.innerWidth;
              return smallGridSizes4371420(windowWidth);
            }
            widthPercent = gp();
            var Resizer = {};

            $(window).resize(function() {
              widthPercent = gp();
              Resizer.gridResizer(widthPercent);
            });

            (Resizer.gridResizer = function (widthPercent) {
              $('#photo-gallery-4371420 .grid .grid-item').each(function(key) {
                pwidth = $(this).data('width');
                pheight = $(this).data('height');
                if(pheight == 0 || pwidth == 0) {
                  pheight = 683;
                  pwidth = 1024;
                }
              
                if (isBlog == 1) {
                  $(this).css({
                    'width' : "calc("+widthPercent+" - "+ gutterValue +"px)"
                  });
                } else {
                  $(this).css({
                   'width' : "calc("+widthPercent+" - "+ gutterValueRes +"px)"
                  });
                }

                $(this).height(parseInt($(this).width() / (pwidth/pheight)));
                $(this).css({
                  'margin-bottom': gutterValue,
                });

              });
            })(widthPercent);

            $grid.isotope({
              itemSelector: '#photo-gallery-4371420 .grid .grid-item',
              percentPosition: true,
              transitionDuration: '0s',
              masonry: {
                columnWidth: '#photo-gallery-4371420 .grid .grid-sizer',
                gutter: gutterValue,                
              }
            });
          };
          gridInit();
        })();
