  function GridMobileSquare4371420(gutterValueRes, gutterValue){      
    var $grid = $('.grid_4371420');
    function gp() {
      var windowWidth = window.innerWidth;
      return mediumGridSizes(windowWidth);
    }
    widthPercent = gp();
    var Resizer = {};
    $(window).resize(function() {
      widthPercent = gp();
      Resizer.gridResizer(widthPercent);
    });
    var g_height = 0;
    (Resizer.gridResizer = function (widthPercent) {
      $('.grid-item_4371420').each(function(){
        $(this).css({
           'width' : "calc("+widthPercent+" - "+ gutterValueRes +"px)"
        });
        
        g_height = $(this).width();
        $(this).height(parseInt(g_height));
        $(this).css({
          'margin-bottom': gutterValue,
        });
      });
    })(widthPercent);
    $grid.isotope({
      itemSelector: '.grid-item_4371420',
      percentPosition: true,
      transitionDuration: '0s',
      masonry: {
        columnWidth: '.grid-sizer_4371420',
        gutter: gutterValue,
      }
    });
  }

  function GridMobileRectangle4371420(gutterValueRes, gutterValue){      
    var $grid = $('.grid_4371420');    
    function gp() {
      var windowWidth = window.innerWidth;
      return mediumGridSizes(windowWidth);
    }
    widthPercent = gp();
    var Resizer = {};
    $(window).resize(function() {
      widthPercent = gp();
      Resizer.gridResizer(widthPercent);
    });
    var g_height = 0;
    (Resizer.gridResizer = function (widthPercent) {
      $('.grid-item_4371420').each(function(){
        $(this).css({
           'width' : "calc("+widthPercent+" - "+ gutterValueRes +"px)"
        });
        if( g_height == 0 )
          g_height = $(this).width()/1.5;

        $(this).height(parseInt(g_height));
        $(this).css({
          'margin-bottom': gutterValue,
        });
      });
    })(widthPercent);
    $grid.isotope({
      itemSelector: '.grid-item_4371420',
      percentPosition: true,
      transitionDuration: '0s',
      masonry: {
        columnWidth: '.grid-sizer_4371420',
        gutter: gutterValue,
      }
    });
  }

// lightGallery start
$(document).ready(function() {
      // feather light video-play
    $('body').on('click', '.js-gallery-video-btn, .js-mediatype-video-play', function(){
      var gallery_video_url = $(this).attr('data-src');
      $('body').append("<div class='featherlight featherlight-iframe u"+
      userObject.userId+"-overlay-bg-color'><div class='featherlight-content'><span class='featherlight-close-icon featherlight-close u"+
      userObject.userId+"-overlay-text-color'><span class='menu-svg-icon'><svg viewBox='0 0 320 512'><use xlink:href='#times-pixpa-icon' width='320' height='512'></use></svg></span></span>" + '<iframe src="'+gallery_video_url+'" style="" class="featherlight-inner js-gallery-video-url"></iframe>' + "</div></div>");
      $(".featherlight").show();
    });

    $('body').on('click', ".featherlight-close-icon", function(){
      $(".featherlight").remove();
    });
  

  window.appendUrlPb = function (currentIndex, linkId, currentGalleryItemId) {  
    //Added for variant url with query string
    let path = new URLSearchParams(window.location.search);
    queryParam = path ? path : '';
    var currentPhotoId = currentGalleryItemId;
    if( userObject.imageUrl != "") {
      history.pushState( {} , 'Image Url', currentPhotoId+'-'+userObject.userId+'-photo-'+currentIndex);
    } else {
      var pathname  = window.location.pathname;
      pathname = pathname.endsWith('/') ? pathname.slice(0, -1) : pathname;
      var splitted = pathname.split('/');
      var x = splitted.length;
      var photomatch = new RegExp('-'+userObject.userId+'-photo-');
      // console.log('photomatch', photomatch, 'match' , splitted[x-1].match(photomatch));
      // if (splitted.join('/').match(/-photo-/)) {
      if (splitted[x-1].match(photomatch)) {
        x = splitted.length - 1;
      }
      splitted = splitted.splice(0, x).join('/');
      if (splitted[splitted.length - 1] != '/') {
        var abc = splitted + '/';
      } else {
        var abc = '/';
      }
      history.replaceState( {} , 'Image Url', abc + currentPhotoId+'-'+userObject.userId+'-photo-'+currentIndex);
    }
    userObject.imageUrl = abc + currentPhotoId+'-'+userObject.userId+'-photo-'+currentIndex;
  }

  window.removeUrlPb = function(){
    var splitted = window.location.pathname.split('/');
    var x = splitted.length;
    var photomatch = new RegExp('-'+userObject.userId+'-photo-');
    // console.log('photomatch', photomatch, 'match' , splitted[x-1].match(photomatch));
    // if (splitted.join('/').match(/-photo-/)) {
    if (splitted[x-1].match(photomatch)) {
      x = splitted.length - 1;
    }
    splitted = splitted.splice(0, x).join('/');
    //Added for variant url with query string
    if(queryParam != '') {
      splitted = splitted + '?' + queryParam;
    }
    history.replaceState( {} , 'Image Url', window.location.origin+splitted);
    userObject.imageUrl = "";
    window.lgCustomIndex = 0;
    // light.obj.index = '';
    $('body').removeClass('lg-overflow-hidden');
    $('body').css({'width': ''}); 
  }

  window.retainStatePb = function() {
    setTimeout(function() {
      if ((galleryonloadType_4371420 == 'grid' || galleryonloadType_4371420 == 'slideshow') && userObject.imageUrl.indexOf('-photo-') >= 0) {
        var url_comp = userObject.imageUrl.split('-photo-');
        if( typeof url_comp == 'object' && typeof url_comp[1] != 'undefined' ) {
          var indx =  parseInt(url_comp[1]);
          if( indx <= window.total_data_4371420.length - 1) {
            // light.init(indx);

            if(onloadType == 'slideshow' && slideshowLayout == "single_image_detailed"){
              $('#photo-gallery-4371420 .ls_slider_img[data-index="'+indx+'"]').click();
            } else if(onloadType == 'slideshow'){              
              $('#photo-gallery-4371420 .js-slideshow-item[data-index="'+indx+'"]').click();
            } else{
              $('#photo-gallery-4371420 .js-grid-item[data-index="'+indx+'"]').click();  
            }
            

            setTimeout(function() {
              if ($('[data-id="'+indx+'"]').hasClass('img-notfound')) {
                $('[data-id="' + indx + '"]').find('.lg-image').attr({
                  'src': window.total_data_4371420[indx].src
                })
              }
            }, 1500);
            
            setTimeout(function() {
              $('body').addClass('lg-overflow-hidden');
              if(window.innerWidth > 1024){
                if (document.documentElement.scrollHeight > document.documentElement.clientHeight){
                  $('body').css({
                    'width': 'calc(100vw - 15px)'
                  });
                } else {
                  $('body').css({
                    'width': '100%'
                  });
                }
              }
            }, 100);
          }
        }
      }
    },1000);
  }
  window.retainStatePb();

  // Esc events captured here
  $(document).keydown(function(e){    
    if (e.keyCode == 27) {
      if($('body').find('.featherlight').hasClass('featherlight')) {
        //Do Noth
      } else if (window.location.pathname.includes('-photo-')) {
        var splitted = window.location.pathname.split('/');
        var x = splitted.length;
        var photomatch = new RegExp('-'+userObject.userId+'-photo-');
        // console.log('photomatch', photomatch, 'match' , splitted[x-1].match(photomatch));
        // if (splitted.join('/').match(/-photo-/)) {
        if (splitted[x-1].match(photomatch)) {
          x = splitted.length - 1;
        }
        splitted = splitted.splice(0, x).join('/');
        //Added for variant url with query string
        if(queryParam != '') {
          splitted = splitted + '?' + queryParam;
        }
        history.replaceState( {} , 'Image Url', window.location.origin+splitted);
        userObject.imageUrl = "";
        
        // history.replaceState( {} , 'Image Url', abc);

        userObject.imageUrl = "";
      } else{
        // console.log('The pathname does not contain -photo-');
      }
    }
  });
  
  setTimeout(function() {
    $(`#dynamic_compnent4371420`).find('.js-custom-section-loader').remove();
  }, 500);
});
// lightGallery end