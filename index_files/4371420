            window.slideIndex = 0;
            var windowHeight, windowWidth, headerHeight, footerHeight;
            windowHeight = window.innerHeight;
            windowWidth = $('body').outerWidth();
            headerHeight = $('.js-full-header').outerHeight();
            if(noOfColumns == 1) {
              if( userObject.isMobile == 1 && !userObject.isTablet ) {
                var value = window.innerHeight, temp = $('#photo-gallery-4371420 #frame #slidee-4371420 li')[0].getAttribute('data-height');
                $.each($('#photo-gallery-4371420 #frame #slidee-4371420 li'), function(index, el) {
                  if( $('#photo-gallery-4371420 #frame #slidee-4371420 li')[index+1] != undefined)
                    if( temp < $('#photo-gallery-4371420 #frame #slidee-4371420 li')[index+1].getAttribute('data-height')) {
                      temp = parseInt($('#photo-gallery-4371420 #frame #slidee-4371420 li')[index+1].getAttribute('data-height'));
                      value = windowWidth/$('#photo-gallery-4371420 #frame #slidee-4371420 li')[index+1].getAttribute('data-aspr');
                    }
                });

                if( value < window.bodyMinHeight)
                  value = window.bodyMinHeight;
                else {
                  value = window.innerHeight;
                }

                var $this = $('#photo-gallery-4371420');
                Global_Pagebuilder.sectionHeight($this);
                $('#photo-gallery-4371420 #frame').outerHeight(sec_height+'vh');

              } else {
                if ($('#photo-gallery-4371420').parents('.is-section-10').length == 1) {
                  var slide_height = '10vh';
                  $('#photo-gallery-4371420 #frame').outerHeight(slide_height);
                } else if ($('#photo-gallery-4371420').parents('.is-section-25').length == 1) {
                  var slide_height = window.innerHeight * 25 / 100;
                  $('#photo-gallery-4371420 #frame').outerHeight(slide_height);
                } else if ($('#photo-gallery-4371420').parents('.is-section-33').length == 1) {
                  var slide_height = window.innerHeight * 33 / 100;
                  $('#photo-gallery-4371420 #frame').outerHeight(slide_height);
                } else if ($('#photo-gallery-4371420').parents('.is-section-50').length == 1) {
                  var slide_height = window.innerHeight * 50 / 100;
                  $('#photo-gallery-4371420 #frame').outerHeight(slide_height);
                } else if ($('#photo-gallery-4371420').parents('.is-section-60').length == 1) {
                  var slide_height = window.innerHeight * 60 / 100;
                  $('#photo-gallery-4371420 #frame').outerHeight(slide_height);
                } else if ($('#photo-gallery-4371420').parents('.is-section-75').length == 1) {
                  var slide_height = window.innerHeight * 75 / 100;
                  $('#photo-gallery-4371420 #frame').outerHeight(slide_height);
                } else if ($('#photo-gallery-4371420').parents('.is-section-90').length == 1) {
                  var slide_height = window.innerHeight * 90 / 100;
                  $('#photo-gallery-4371420 #frame').outerHeight(slide_height);
                } else if ($('#photo-gallery-4371420').parents('.is-section-100').length == 1) {
                  var slide_height = window.innerHeight * 100 / 100;
                  $('#photo-gallery-4371420 #frame').outerHeight(slide_height);
                } else{
                  var slide_height = '100vh';
                  $('#photo-gallery-4371420').css({
                    'min-height' : '100vh'
                  });
                  $('#photo-gallery-4371420').height('100vh');
                  $('#photo-gallery-4371420 #frame').height('100vh');
                }
              }
            } else {
              var $this = $('#photo-gallery-4371420');
              Global_Pagebuilder.sectionHeight($this); // global pagebuilder.js 
              $('#photo-gallery-4371420 #frame').outerHeight(sec_height+'vh');
              if (sec_height_blank == 100) {
                $('#photo-gallery-4371420').css({
                  'min-height': sec_height+'vh',
                  'height': sec_height+'vh',
                });
                $('#photo-gallery-4371420 #frame').height(sec_height+'vh');
              }
            }

            var temp = $($('#photo-gallery-4371420 #frame #slidee-4371420 li')[0]);
            temp.removeClass('js-horizontal_slider-destroy');
            var temp1 = $('#photo-gallery-4371420 #frame #slidee-4371420 li');
            var sum = 0, dynamicUrl, a = 0, DPR = window.devicePixelRatio;
            if(noOfColumns == 1) {
            } else{
              windowWidth = windowWidth - 250;
            }

            $.each($('#photo-gallery-4371420 #frame #slidee-4371420 li'), function(index, el) {
              var pwidth = $(el).data('width');
              var pheight = $(el).data('height');
              if( pheight == 0 || pwidth == 0 ) {
                pheight = 683;
                pwidth = 1024;
              }
              var height = $(el).height();
              var width = height*(pwidth/pheight);
              if((width+100) > windowWidth) {
                if(windowWidth < 767)
                  var w = windowWidth;
                else
                  var w = windowWidth-150;

                $(el).width(w);
                $(el).height(w*(pheight/pwidth));
              }
              else {
                $(el).width(width);
                $(el).children('img').width();
              }
              sum = sum + $(el).width() + 1;
            });
            
            sum = sum + 1;

            // Active 1st Object
            temp1.removeClass('js-slideshow-item');
            temp.addClass('list-active js-slideshow-item');
            temp1.find('.js-hoz-photo-info-btn, .js-photo-info-wrapper').removeClass('active');
            temp.find('.js-hoz-photo-info-btn').addClass('active');
