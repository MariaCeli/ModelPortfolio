                $(this).find('.horizontal_img').attr('src', $(this).find('.horizontal_img').attr('data-src'));
                $(this).find('.horizontal_img').css('display', 'initial');
              });
            } else {
              $('#photo-gallery-4371420 #frame #slidee-4371420 li').slice(0, 5).each(function(index, el) {
                $(this).find('.horizontal_img').attr('src', $(this).find('.horizontal_img').attr('data-src'));
                $(this).find('.horizontal_img').css('display', 'initial');
              });

              var dragging = false;
              $("#photo-gallery-4371420 #frame #slidee-4371420").on("mousedown", function(e) {
                var x = e.screenX;
                var y = e.screenY;
                dragging = false;
                $("#photo-gallery-4371420 #frame #slidee-4371420").on("mousemove", function(e) {
                  if (Math.abs(x - e.screenX) > 5 || Math.abs(y - e.screenY) > 5) {
                    dragging = true;
                    $('#photo-gallery-4371420 #frame #slidee-4371420 li').each(function(index, el) {
                      if ($(this).find('.horizontal_img').attr('src') == '') {
                        $(this).find('.horizontal_img').attr('src', $(this).find('.horizontal_img').attr('data-src'));
                        $(this).find('.horizontal_img').css('display', 'initial');
                      }
                    });
                  }
                });
              });

              $("#photo-gallery-4371420 #frame #slidee-4371420").on("mouseup", function(e) {
                $("#frame #slidee-4371420").off("mousemove");
                $('#photo-gallery-4371420 #frame #slidee-4371420 li').each(function(index, el) {
                  if ($(this).find('.horizontal_img').attr('src') == '') {
                    $(this).find('.horizontal_img').attr('src', $(this).find('.horizontal_img').attr('data-src'));
                    $(this).find('.horizontal_img').css('display', 'initial');
                  }
                });
              });
            }
            
            $frame.sly('on', 'active', function (eventName, itemIndex) {
              window.slideIndex = itemIndex;
              var temp = '#photo-gallery-4371420 #fl3_'+ itemIndex;
              Global_Pagebuilder.sliderGalleryImgProxyUrl(temp, itemIndex, 4371420);

              $('body').addClass('lg-overflow-horizontal-hidden'); // lightgallery class add in body for stop flickering
