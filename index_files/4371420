                    $el.parents('.single-image-slider').find('.js-lgs-photo-info').attr('id', 'js-lgsphoto-info-btn-'+currentIndex);
                    $el.parents('.single-image-slider').find('.js-lgs-photo-info').attr('data-photoinfo' , setlgsphotoinfo);
                    $el.parents('.single-image-slider').find('.js-lgs-photo-info').attr('data-photodesc' , setlgsphotodesc);
                  }

                  if($el.find('.lslide.active').attr('data-mediatype') == 3){
                    $('.js-lS-caption').hide();
                  } else {
                    $('.js-lS-caption').show();
                  }
                }, 150)
              }
