            temp.next().addClass('next_slide');
            var hozslideshowSpeed = "3";
            var $frame = $('#photo-gallery-4371420 #frame');
            var options = {
              horizontal: 1,
              smart: 1,
              activateOn: 'click', // can be click or mouseenter
              itemNav: 'centered',
              speed: (parseInt(hozslideshowSpeed)*1000)*0.25,
              activateMiddle: true,  // works only when itemnav is forcedCentered
              mouseDragging: 1,
              touchDragging: 1,
              releaseSwing: 1,
              startAt: 0,
              cycleBy: 'items',
              cycleInterval: (parseInt(hozslideshowSpeed)*1000),
              pauseOnHover: true,
              elasticBounds: 1,
              keyboardNavBy: true,
              moveBy: (parseInt(hozslideshowSpeed)*1000),
              easing: 'swing', // Default Swing and Linear for more use jquery easing plugin
              activeClass: 'list-active',  // Class for active items and pages.
            };

            if(windowWidth < 600) {
              temp1.css('opacity',1);
            }

            $frame.sly(options);
            // Needed not accepting a little overflow
            $('#photo-gallery-4371420 .slidee').width(sum+1+slideWidthWithGutter);
