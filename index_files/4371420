        }
      });
    }
  }

  function getNextPageData_4371420() {
    console.log('fetching next page....');
    var nextData = {};
    var j = 0;
    window.nextKey_4371420 = window.nextKey_4371420 || parseInt($($('.js-lastGridItemid_4371420')[$('.js-lastGridItemid_4371420').length-1]).attr('data-key'));
    $.each(window.total_data_4371420, function(i,v) {
      if(i > window.nextKey_4371420) {
        j++;
        nextData[window.nextKey_4371420+j] = v;
        if(j >= 48)
          return false;
      }
    });
    window.nextKey_4371420 += j;
    // console.log({ 0 : nextData, 1 : "success" });
    return { 0 : nextData, 1 : "success" };
  }

  function unveilIEImages() {
    //var ms_ie = false;
    var ua = window.navigator.userAgent;
    var old_ie = ua.indexOf('MSIE ');
    var new_ie = ua.indexOf('Trident/');
    if ((old_ie > -1) || (new_ie > -1)) {
      $('.lazyload').each(function( i,v ) {
        console.log('IE detected')
        lazySizes.loader.unveil(this);
      });
    }
  }

  unveilIEImages();
