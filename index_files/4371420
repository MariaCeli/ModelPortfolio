                $el.parents('.single-image-slider').find('.js-lgs-photo-info').attr('id', 'js-lgsphoto-info-btn-'+currentIndex);
                $el.parents('.single-image-slider').find('.js-lgs-photo-info').attr('data-photoinfo' , lgsphotoinfo);
                $el.parents('.single-image-slider').find('.js-lgs-photo-info').attr('data-photodesc' , lgsphotodesc);
