  function applyAccentOverlay(){
    $("#photo-gallery-4371420 .js-image-overlay ").each(function(){
      if(!$(this).data("overlay_done")){
        var color = $(this).css("background-color");
        var opacity = $(this).css("order");
        var gridLayout = "portrait_large";
        if(color == 'rgba(0, 0, 0, 0)') {
          let rgb = $(this).parent().css("background-color");
          rgb = getRgbaColor(rgb,(opacity*0.01));

          $(this).css("background-color",rgb);
          // $(this).attr('data-overlay_done',true); 

        }
        var base_color = $(this).css("color");
        var opacity1 = $(this).css("orphans");
        if(base_color != 'rgb(0, 0, 0)'){
          if (gridLayout == 'square_block_grid' || gridLayout == 'three_two_grid') {
          } else{
            $(this).parent().css("background-color",base_color);
          }        
        }else{
          base_color = $(this).parent().css("background-color");
          base_color  = getRgbaColor(base_color,(opacity1*0.01));          
          if (gridLayout == 'square_block_grid' || gridLayout == 'three_two_grid') {
          } else{
            $(this).parent().css("background-color",base_color);
          }
        }
      }
    });
  }

  function applyGridBorderRadius(){
    var grid_border_radius = "0";
    var onloadType = "grid";
    var gridLayout = "portrait_large";

    if(global_override_width == '0') {
      grid_border_radius = "0";
    } else {
      grid_border_radius = "0";
    }
    
    if (onloadType == 'grid') {
      $("#photo-gallery-4371420 .js-grid-item" ).each(function(){
        if (onloadType == 'grid' && (gridLayout == 'pinterest_grid_detailed' || gridLayout == 'pinterest_grid_large_detailed' || gridLayout == 'square_block_grid' || gridLayout == 'three_two_grid')) {
          if( window.innerWidth <= 767 && (gridLayout == 'square_block_grid' || gridLayout == 'three_two_grid')) {
            $(this).css("border-radius", grid_border_radius+'px');
            $(this).css("overflow", 'hidden');
          } else{
            $(this).find('.js-grid-image').css("border-radius", grid_border_radius+'px');
            $(this).find('.js-grid-image').css("overflow", 'hidden');
          }
        } else if(onloadType == 'grid' && (gridLayout == 'spaced_out_small' || gridLayout == 'spaced_out_medium' || gridLayout == 'spaced_out_large')){
          $(this).find('figure').css("border-radius", grid_border_radius+'px');
          $(this).find('figure').css("overflow", 'hidden');          
        } else{
          $(this).css("border-radius", grid_border_radius+'px');
          $(this).css("overflow", 'hidden');
        }
      });
    } else if (onloadType == 'slideshow') {
      if( galleryslideshowlayout_4371420 == 'vertical_slider' || galleryslideshowlayout_4371420 == 'vertical_slider_detailed' ) {
        $("#photo-gallery-4371420 .slideshow-item img" ).each(function(){
          $(this).css("border-radius", grid_border_radius+'px');
          $(this).css("overflow", 'hidden');
        });
      } else if( galleryslideshowlayout_4371420 == 'vertical_slider_offset' ) {
        $("#photo-gallery-4371420 .js-slideshow-image-container" ).each(function(){
          $(this).css("border-radius", grid_border_radius+'px');
          $(this).css("overflow", 'hidden');
        });
      } else if( galleryslideshowlayout_4371420 == 'horizontal_slider' || galleryslideshowlayout_4371420 == 'horizontal_scroll') {
        $("#photo-gallery-4371420 #frame .slidee li .horizontal_img" ).each(function(){
          $(this).css("border-radius", grid_border_radius+'px');
          $(this).css("overflow", 'hidden');
        });
      }
    }
    
   $('.mediatype-video-play img').css("border-radius", '0px');

  };

  function getRgbaColor(color,opacity){
    if(color.indexOf('a') == -1){
      return  color.replace(')', ','+opacity+')').replace('rgb', 'rgba');
    }
  }
  function applyGridBackgroundColor(){
    var grid_bg_color = "0";
    if(grid_bg_color  != 0){
      $("#photo-gallery-4371420 .js-grid-item" ).each(function(){
        if(!$(this).data("gridbg_done")){
          $(this).css("background-color",grid_bg_color);
          $(this).attr('data-gridbg_done',true);
        }
      });
    }
  }

  layout_mobile_4371420 = "2";

  function smallGridSizes4371420(w) {
    w = window.innerWidth;

    if(userObject.deviceType == 'desktop'){
      if( w > 2500 ) return '12.5%';
      else if( w > 1925 && w <= 2500 ) return '14.28%';
      else if( w > 1200 && w <= 1925 ) return '20%';
      else if( w > 1024 && w <= 1200 ) return '25%';
      else return '100%';  
    } else {
      if(layout_mobile_4371420 == 1 && w >= 768 && w <= 1024 && window.matchMedia("(orientation: portrait)").matches) return '50%';
      else if(layout_mobile_4371420 == 1 && w >= 768 && w < 1024 && window.matchMedia("(orientation: landscape)").matches) return '50%';
      else if(layout_mobile_4371420 == 1 && w >= 1024 && w <= 1366 && window.matchMedia("(orientation: landscape)").matches) return '33.333%';        
      else if(layout_mobile_4371420 == 1 && window.matchMedia("(orientation: portrait)").matches) return '100%';
      else if(layout_mobile_4371420 == 1 && window.matchMedia("(orientation: landscape)").matches) return '50%';

      else if(layout_mobile_4371420 == 2 && w >= 768 && w <= 1024 && window.matchMedia("(orientation: portrait)").matches) return '50%';
      else if(layout_mobile_4371420 == 2 && w >= 768 && w < 1024 && window.matchMedia("(orientation: landscape)").matches) return '33.333%';
      else if(layout_mobile_4371420 == 2 && w >= 1024 && w <= 1366 && window.matchMedia("(orientation: landscape)").matches) return '33.333%';
      else if(layout_mobile_4371420 == 2 && window.matchMedia("(orientation: portrait)").matches) return '50%';
      else if(layout_mobile_4371420 == 2 && window.matchMedia("(orientation: portrait)").matches) return '50%';
      else if(layout_mobile_4371420 == 2 && window.matchMedia("(orientation: landscape)").matches) return '33.33%';
      
      else if(layout_mobile_4371420 == 3) return '33%';
      else return '100%';
    }
  }


  function smallGridSizes(w) {
    w = window.innerWidth;
    if( w > 2500 ) return '12.5%';
    else if( w > 1925 && w <= 2500 ) return '14.28%';
    else if( w > 1200 && w <= 1925 ) return '20%';
    else if( w > 767 && w <= 1200 ) return '33.33%';
    else if(layout_mobile_4371420 == 1) return '100%';
    else if(layout_mobile_4371420 == 2) return '50%';
    else if(layout_mobile_4371420 == 3) return '33.33%';
    else return '100%';
  }

